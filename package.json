{
  "name": "shadcnui",
  "type": "module",
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": "20.17.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "node server.js",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --max-semi-space-size=2048\" vite build",
    "build:safe": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --gc-interval=100\" vite build",
    "build:minimal": "vite build --minify=false --sourcemap=false",
    "build:simple": "vite build",
    "build:emergency": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048\" vite build --minify=false --sourcemap=false",
    "build:heroku-safe": "NODE_ENV=production vite build --mode production",
    "build:no-chunks": "vite build --minify=false --sourcemap=false",
    "build:windows": "vite build",
    "start": "node server.js",
    "start:dev": "serve -s dist -p 3000 --host 0.0.0.0",
    "preview": "vite preview",
    "heroku-postbuild": "npm run build:emergency",
    "digitalocean-postbuild": "npm run build:simple",
    "health": "curl http://localhost:8080/api/health 2>/dev/null || echo 'Server not responding'",
    "check-env": "node -e \"console.log('Environment Check:'); console.log('GOOGLE_CLIENT_ID:', process.env.GOOGLE_CLIENT_ID ? 'Set' : 'Missing'); console.log('GOOGLE_CLIENT_SECRET:', process.env.GOOGLE_CLIENT_SECRET ? 'Set' : 'Missing'); console.log('NODE_ENV:', process.env.NODE_ENV || 'Not set');\"",
    "security-fix": "npm audit fix --force",
    "lint": "eslint --quiet ./src",
    "check-deprecated": "grep -r \"url\\.parse\" . --exclude-dir=node_modules --exclude-dir=dist || echo 'No deprecated url.parse() found'"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-oauth/google": "^0.12.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-query": "^5.56.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "embla-carousel-react": "^8.3.0",
    "express": "^4.19.2",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "serve": "^14.2.4",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.11",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "vaul": "^0.9.3",
    "vite": "^5.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "lovable-tagger": "^1.1.7",
    "typescript-eslint": "^8.0.1"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "on-headers@<1.1.0": ">=1.1.0"
    }
  }
}
